{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "/order", "function": "order" }
    ],
    "redirects": [
      { "source": "/www.eliteunlock.shop/**", "destination": "https://eliteunlock.shop", "type": 301 }
    ]
  }
}
